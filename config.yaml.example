# Natural Language SQL Interface Configuration
# Example configuration for organization AI model

# Database Configuration
database:
  host: localhost
  port: 5432
  username: your_db_username
  password: your_db_password
  database: your_database_name
  schema: public

# Organization AI Model Configuration
ai_model:
  client_id: your_client_id_here          # Replace with your actual client ID
  client_secret: your_client_secret_here  # Replace with your actual client secret
  base_url: your_base_url_here           # Replace with your actual base URL
  model_name: llama-3-3-70b-instruct     # Your organization's model name

# Rate Limiting Configuration
rate_limit:
  max_requests: 60       # Maximum requests per window
  window_seconds: 60     # Time window in seconds
  burst_limit: 10        # Maximum burst requests

# Application Configuration
app:
  host: 0.0.0.0
  port: 3000
  debug: false
  secret_key: your-production-secret-key-here